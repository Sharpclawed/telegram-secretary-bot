{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithProcessId", "WithThreadId"], 
    "WriteTo": [
        {"Name": "Console" },
        {
          "Name": "File",
          "Args": {
            "path": "C:\\TelegramBot\\logs\\log-.json",
            "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
            "rollingInterval": "Day"
          }
        }
      ] 
  },
  "AllowedHosts": "*"
}
