  <connectionStrings>
    <add name="DBConnection" connectionString="provider=Npgsql;provider connection string=&quot;Database=TelegramHistory;Password=postgres;User Id=postgres;Server=localhost&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
</configuration>
